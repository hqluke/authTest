<html>
    <head>
        <title>Calendar</title>
    </head>
    <body>
        <h1>Calendar</h1>
        <table>
            <thead>
                <tr>
                    <th>Date</th>
                    <th>Exercise</th>
                    <th>Sets</th>
                    <th>Reps</th>
                    <th>Weight</th>
                </tr>
            </thead>
            <tbody>
                <% calendar.forEach(function(row) { %>
                <tr>
                    <td><%= row.date.toLocaleDateString() %></td>
                    <td><%= row.name %></td>
                    <td><%= row.sets %></td>
                    <td><%= row.reps %></td>
                    <td><%= row.weight %></td>
                </tr>
                <% }) %>
            </tbody>
        </table>
    </body>
</html>
